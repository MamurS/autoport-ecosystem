{
  "name": "autoport-telegram",
  "version": "1.0.0",
  "description": "AutoPort Telegram Mini App and Bot",
  "main": "bot/bot.js",
  "scripts": {
    "start": "node bot/bot.js",
    "dev": "nodemon bot/bot.js",
    "serve-webapp": "npx http-server webapp -p 8080 -c-1",
    "mock-api": "node scripts/mock-server.js",
    "test": "node scripts/test-api.js",
    "deploy": "npm run build && gh-pages -d webapp"
  },
  "dependencies": {
    "node-telegram-bot-api": "^0.64.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "http-server": "^14.1.1",
    "gh-pages": "^6.0.0"
  }
} 